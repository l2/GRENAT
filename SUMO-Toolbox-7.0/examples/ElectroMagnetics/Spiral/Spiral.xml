<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>Spiral</Name>
		<Description>
			Electromagnetic simulation data for a spiral
			inductance in function of frequency and two
			shape parameters.
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="SideLength" type="real"/>
			<Parameter name="PortWidth" type="real"/>
			<Parameter name="Frequency" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="S11" type="complex"/>
			<Parameter name="S12" type="complex"/>
		</OutputParameters>
		
		<Implementation>
			<DataFiles>
				<GriddedDataFile gridsize="50,25,75">SpiralGrid</GriddedDataFile>
			</DataFiles>

		</Implementation>
	</Simulator>
