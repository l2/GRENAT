<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>Gap</Name>
		<Description>
			Electromagnetic simulations of a gap in a
			microstrip line. Outputs are the scattering
			parameters.
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="Width" type="real"/>
			<Parameter name="s" type="real"/>
			<Parameter name="Frequency" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="S11" type="complex"/>
			<Parameter name="S12" type="complex"/>
		</OutputParameters>
		
		<Implementation>			
			<DataFiles>
				<GriddedDataFile gridsize="20,20,40">GapGrid</GriddedDataFile>
			</DataFiles>
		
		</Implementation>
	</Simulator>
