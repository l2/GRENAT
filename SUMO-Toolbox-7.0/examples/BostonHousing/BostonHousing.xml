<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>BostonHousing</Name>
	<Description>
		The Boston Housing Dataset
		A standard benchmark dataset from the UCI repository
		http://www.ailab.si/orange/doc/datasets/housing.htm
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="CRIM" type="real"/>
		<Parameter name="ZN" type="real"/>
		<Parameter name="INDUS" type="real"/>
		<Parameter name="CHAS" type="real"/>
		<Parameter name="NOX" type="real"/>
		<Parameter name="RM" type="real"/>
		<Parameter name="AGE" type="real"/>
		<Parameter name="DIS" type="real"/>
		<Parameter name="RAD" type="real"/>
		<Parameter name="TAX" type="real"/>
		<Parameter name="PTRATIO" type="real"/>
		<Parameter name="B" type="real"/>
		<Parameter name="LSTAT" type="real"/>
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="MEDV" type="real"/>
	</OutputParameters>
		
	<!-- A simulator may have multiple implementations: as an executable, a 
	java main class, a dataset, ...-->
	<Implementation>
		<DataFiles>
			<ScatteredDataFile id="default">BostonHousing</ScatteredDataFile>
		</DataFiles>
	</Implementation>
</Simulator>

