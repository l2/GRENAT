<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>NeuralNet3D</Name>
	<Description>
		A Neural network with 3 hidden layers with random, fixed weights
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real"/>
		<Parameter name="y" type="real"/>
		<Parameter name="z" type="real"/>
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
		
	<Implementation>
		<Executables>
			<Executable platform="matlab">NeuralNet3D</Executable>
		</Executables>
		<DataFiles>
			<ScatteredDataFile id="default">NeuralNet3DScattered</ScatteredDataFile>
			<ScatteredDataFile id="test">NeuralNet3DScattered2</ScatteredDataFile>
		</DataFiles>
	</Implementation>
</Simulator>

