<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>vlmop2</Name>
	<Description>
		Veldhuizen and Lamontâ€™s MOP2 function.
		Scalable in the number of decision variables. We use just two

		The Pareto front is concave and the Pareto optima lie on the
		diagonal passing from -1/sqrt(n),-1sqrt(n) to 1/sqrt(n),1/sqrt(n) in
		parameter space.
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x1" type="real" minimum="-2" maximum="2"/>
		<Parameter name="x2" type="real" minimum="-2" maximum="2"/>
	</InputParameters>
	
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="f1" type="real"/>
		<Parameter name="f2" type="real"/>
	</OutputParameters>
	
	<!-- A simulator may have multiple implementations: as an executable, a 
	     java main class, a dataset, ...-->
	<Implementation>
		
		<Executables>
			<!-- An arch attribute may make sense here as well -->
			<Executable platform="matlab">vlmop2</Executable>
		</Executables>
      
	</Implementation>
</Simulator>

