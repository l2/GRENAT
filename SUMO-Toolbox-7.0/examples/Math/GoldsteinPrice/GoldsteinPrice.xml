<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>GoldsteinPrice</Name>
	<Description>
		Goldstein-Price's function
        Several local minimum. Global minimum: f(0, -1) = 3
		Note: this function is prescaled from [-2,2] to M3 space, so global minimum is at (0,-0.5)
	</Description>
		
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real" />
		<Parameter name="y" type="real" />
	</InputParameters>
	
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
	
	<!-- A simulator may have multiple implementations: as an executable, a 
	     java main class, a dataset, ...-->
	<Implementation>
		
		<Executables>
			<!-- An arch attribute may make sense here as well -->
			<Executable platform="java" classpath="">ibbt.sumo.examples.GoldsteinPrice</Executable>
			<Executable platform="matlab">GoldsteinPrice</Executable>
		</Executables>
		<DataFiles>
			<ScatteredDataFile id="default">GoldsteinPriceScattered.txt</ScatteredDataFile>
		</DataFiles>
		
	</Implementation>
</Simulator>
	
