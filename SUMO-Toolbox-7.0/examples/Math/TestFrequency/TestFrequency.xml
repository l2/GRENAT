<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Test Frequency</Name>
	<Description>
		Test problem for frequency sampling.
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real" minimum="0" maximum="1"/>
		<Parameter name="f" type="real" autoSampling="true" />
		
		<Constraint type="NonlinearConstraint">
			<Option key="f" value="TestFrequencyConstraint"/>
		</Constraint>
		
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
	
	<!-- A simulator may have multiple implementations: as an executable, a 
	java main class, a dataset, ...-->
	<Implementation>
		
		<Executables>
			<Executable platform="matlab" batch="true" batchSize="2">TestFrequency</Executable>
		</Executables>
	
	</Implementation>
	
</Simulator>

