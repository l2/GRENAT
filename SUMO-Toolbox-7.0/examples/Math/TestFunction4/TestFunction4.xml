<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>TestFunction4</Name>
	<Description>
		Test function from the paper
		D. Busby, C.L. Farmer, and A. Iske:
		Hierarchical Nonlinear Approximation for Experimental Design and Statistical Data Fitting.
		SIAM Journal on Scientific Computing, 29(1), 2007, 49-69.
 	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real"/>
		<Parameter name="y" type="real"/>
		<Parameter name="z" type="real"/>
	</InputParameters>
	
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
	
	<Implementation>
		
		<Executables>
			<Executable platform="java" classpath="">ibbt.sumo.examples.TestFunction4</Executable>
			<Executable platform="matlab" classpath="">TestFunction4</Executable>
		</Executables>
		<DataFiles>			
			<ScatteredDataFile id="default">TestFunction4Scattered</ScatteredDataFile>
			<ScatteredDataFile id="validation">TestFunction4Scattered2</ScatteredDataFile>
		</DataFiles>
		
	</Implementation>
</Simulator>
