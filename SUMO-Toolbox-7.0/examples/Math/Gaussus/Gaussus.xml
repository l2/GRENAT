<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Gaussus</Name>
	<Description>
	A function consisting of three other functions:
		- a plane
		- a gauss function
		- a sine function		 
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real"/>
		<Parameter name="y" type="real"/>
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
		
	<Implementation>
			
		<Executables>
			<Executable platform="java" classpath="">ibbt.sumo.examples.GaussusSimulator</Executable>
			<Executable platform="unix" arch="x86">Gaussus</Executable>
		</Executables>

		
	</Implementation>
</Simulator>

