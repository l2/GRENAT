<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>Kotanchek</Name>
		<Description>
			A 5-input, 2-output test function.
			The output only depends on the first two inputs,
			which makes this function (and especially the scattered
			data set) an excellent testcase for adaptive modelling
			with rational functions (as the weights for the
			other three parameters should tend to infinity).
			The first output is without added noise, the second one
			with added noise.
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="X" type="real"/>
			<Parameter name="Y" type="real"/>
			<Parameter name="U1" type="real"/>
			<Parameter name="U2" type="real"/>
			<Parameter name="U3" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="exact" type="real"/>
			<Parameter name="noisy" type="real"/>
		</OutputParameters>
		
		<Implementation>
			
			<Executables>
				<Executable platform="java" classpath="">ibbt.sumo.examples.KotanchekSimulator</Executable>
				<Executable platform="matlab">Kotanchek</Executable>
			</Executables>
			<DataFiles>
				<ScatteredDataFile id="original">KotanchekScattered</ScatteredDataFile>
				<ScatteredDataFile id="default">KotanchekScattered2</ScatteredDataFile>
				<ScatteredDataFile id="validation">KotanchekScattered3</ScatteredDataFile>
			</DataFiles>
		
		</Implementation>
	</Simulator>
