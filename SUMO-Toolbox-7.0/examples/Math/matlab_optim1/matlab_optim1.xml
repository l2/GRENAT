<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>matlab_optim1</Name>
		<Description>
			Origin is in matlab optimization toolbox (large-scale optimization)
			Used by Gary Wang rbf_hdmr paper
			
			See section "Example: Nonlinear Minimization with Gradient and Hessian"

			2D version
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="x1" type="real" minimum="0" maximum="1" />
			<Parameter name="x2" type="real" minimum="0" maximum="1" />
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="y" type="real"/>
		</OutputParameters>
			
		<!-- A simulator may have multiple implementations: as an executable, a 
		java main class, a dataset, ...-->
		<Implementation>
			
			<Executables>
				<Executable platform="matlab">matlab_optim1</Executable>
			</Executables>
		
		</Implementation>
	</Simulator>
