<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>StepFunction</Name>
		<Description>
			A function with many discontinuous steps.
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="x" type="real"/>
			<Parameter name="y" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="out" type="real"/>
		</OutputParameters>
			
		<Implementation>
			
			<Executables>
				<Executable platform="java" classpath="">ibbt.sumo.examples.StepSimulator</Executable>
			</Executables>
			<DataFiles>
 				<ScatteredDataFile>StepFunctionScattered.txt</ScatteredDataFile>
			</DataFiles>
		
		</Implementation>
	</Simulator>
