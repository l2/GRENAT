<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>Ackley</Name>
		<Description>
			Ackley's Path [Ack87] is a widely used multimodal test function
			3D version
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="x1" type="real"/>
			<Parameter name="x2" type="real"/>
			<Parameter name="x3" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="y" type="real"/>
		</OutputParameters>
			
		<Implementation>
			
			<Executables>
				<Executable platform="matlab">Ackley</Executable>
			</Executables>
			
			<DataFiles>
				<GriddedDataFile id="test" gridsize="20,20,20">Ackley3DGrid</GriddedDataFile>
			</DataFiles>
		
		</Implementation>
	</Simulator>
