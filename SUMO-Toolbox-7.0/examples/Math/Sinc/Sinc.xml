<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>Sinc</Name>
		<Description>
			A 2D Sinc function
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="x1" type="real"/>
			<Parameter name="x2" type="real"/>
			<Parameter name="x3" type="real"/>
			<Parameter name="x4" type="real"/>
			<Parameter name="x5" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="out" type="real"/>
		</OutputParameters>
			
		<Implementation>
			
			<Executables>
				<Executable platform="java" classpath="">ibbt.sumo.examples.SincSimulator</Executable>
			</Executables>
			<DataFiles>
			</DataFiles>
		
		</Implementation>
	</Simulator>
