<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Shekel</Name>
	<Description>
		Shekel's foxholes
		See: http://www.ewh.ieee.org/soc/es/May2001/14/Begin.htm
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real"/>
		<Parameter name="y" type="real"/>
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
		
	<Implementation>
		<Executables>
			<Executable platform="unix" arch="x86">Shekel</Executable>
			<Executable platform="unix" arch="amd64">Shekel</Executable>
		</Executables>
		
		<DataFiles>
			<ScatteredDataFile>Shekel</ScatteredDataFile>
		</DataFiles>
		
	</Implementation>
</Simulator>

