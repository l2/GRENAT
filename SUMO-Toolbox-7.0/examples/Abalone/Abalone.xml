<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Abalone</Name>
	<Description>
		This dataset comes from the UCI repository of machine learning databases. 
		The task is to predict the age (number of rings) of abalone (some sort of shellfish) from physical measurements. 
		http://www.cs.toronto.edu/~delve/data/abalone/desc.html
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="sex" type="real"/>
		<Parameter name="length" type="real"/>
		<Parameter name="diameter" type="real"/>
		<Parameter name="height" type="real"/>
		<Parameter name="whole_weight" type="real"/>
		<Parameter name="shucked_weight" type="real"/>
		<Parameter name="viscera_weight" type="real"/>
		<Parameter name="shell_weight" type="real"/>
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="rings" type="real"/>
	</OutputParameters>
		
	<!-- A simulator may have multiple implementations: as an executable, a 
	java main class, a dataset, ...-->
	<Implementation>
		<DataFiles>
			<ScatteredDataFile>Abalone</ScatteredDataFile>
		</DataFiles>
	</Implementation>
</Simulator>

