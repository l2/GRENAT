<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Laser</Name>
	<Description>
		The laser dataset from the Santa Fe Timeseries prediction competition.
		http://www-psych.stanford.edu/~andreas/Time-Series/SantaFe.html
 	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="x" type="real"/>
	</InputParameters>
	
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
	
	<Implementation>
		
		<DataFiles>
			<ScatteredDataFile id="default">Laser</ScatteredDataFile>
		</DataFiles>
		
	</Implementation>
</Simulator>
