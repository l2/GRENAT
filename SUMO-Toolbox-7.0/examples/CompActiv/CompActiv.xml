<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>CompActiv</Name>
	<Description>
		The computerActivity database records various performance measures, such as the bytes read/written from sytem memory,
		from a Sun Sparctation 20/712 with 2 cpus and 128 Mbyte of main memory.
		Source: http://www.cs.toronto.edu/~delve/data/comp-activ/desc.html
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="lread" type="real"/>
		<Parameter name="lwrite" type="real"/>
		<Parameter name="scall" type="real"/>
		<Parameter name="sread" type="real"/>
		<Parameter name="swrite" type="real"/>
		<Parameter name="fork" type="real"/>
		<Parameter name="exec" type="real"/>
		<Parameter name="rchar" type="real"/>
		<Parameter name="wchar" type="real"/>
		<Parameter name="pgout" type="real"/>
		<Parameter name="ppgout" type="real"/>
		<Parameter name="pgfree" type="real"/>
		<Parameter name="pgscan" type="real"/>
		<Parameter name="atch" type="real"/>
		<Parameter name="pgin" type="real"/>
		<Parameter name="ppgin" type="real"/>
		<Parameter name="pflt" type="real"/>
		<Parameter name="vflt" type="real"/>
		<Parameter name="runqsz" type="real"/>
		<Parameter name="freemem" type="real"/>
		<Parameter name="freeswap" type="real"/>
	</InputParameters>
		
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="usr" type="real"/>
	</OutputParameters>
		
	<Implementation>
		<DataFiles>
			<ScatteredDataFile id="default">CompActiv</ScatteredDataFile>
		</DataFiles>
	</Implementation>
</Simulator>

