<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Skull</Name>
	<Description>
		Model a Skull
		Data from Luigi Giaccari - http://www.advancedmcode.org/ 
	</Description>
	
	<InputParameters>
		<Parameter name="x" 	type="real" min="-5.88" max="9.7"/>
		<Parameter name="y"	type="real" min="-7.45" max="10.7"/>
		<Parameter name="z"	type="real" min="-12.54" max="8.87"/>
	</InputParameters>
		
	<OutputParameters>
		<Parameter name="in" type="real" min="-1" max="1"/>
	</OutputParameters>
	
	<Options>
	  <Option key="model" value="Skull.mat"/>
	</Options>
	
	<Implementation>
		<Executables>
			<Executable platform="matlab" batch="true" batchSize="500">inModel</Executable>
		</Executables>
		
	</Implementation>
</Simulator>
