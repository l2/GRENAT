<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Cactus</Name>
	<Description>
		Cactus model
		Data from Luigi Giaccari - http://www.advancedmcode.org/ 
	</Description>
	
	<InputParameters>
		<Parameter name="x" 	type="real" min="0.4339" max="0.5661"/>
		<Parameter name="y"	type="real" min="0.2467" max="0.7533"/>
		<Parameter name="z"	type="real" min="0.1" max="0.9"/>
	</InputParameters>
		
	<OutputParameters>
		<Parameter name="in" type="real" min="-1" max="1"/>
	</OutputParameters>
	
	<Options>
	  <Option key="model" value="Cactus.mat"/>
	</Options>
	
	<Implementation>
		<Executables>
			<!--Executable platform="matlab" batchMode="true" batchSize="100">inModel</Executable-->
			<Executable platform="matlab" batch="true" batchSize="100">inModel</Executable>
		</Executables>
	</Implementation>
</Simulator>
