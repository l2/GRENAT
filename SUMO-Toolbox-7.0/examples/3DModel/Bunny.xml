<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Bunny</Name>
	<Description>
		The Stanford Bunny
		Data from Luigi Giaccari - http://www.advancedmcode.org/ 
	</Description>

	<InputParameters>
		<Parameter name="x" 	type="real" min="-0.094" max="0.062"/>
		<Parameter name="y"	type="real" min="0.033" max="0.1874"/>
		<Parameter name="z"	type="real" min="-0.0619" max="0.0589"/>
	</InputParameters>
		
	<OutputParameters>
		<Parameter name="in" type="real" min="-1" max="1"/>
	</OutputParameters>
	
	<Options>
	  <Option key="model" value="Stanford_Bunny.mat"/>
	</Options>
	
	<Implementation>
		<Executables>
			<Executable platform="matlab" batch="true" batchSize="500">inModel</Executable>
		</Executables>
		
	</Implementation>
</Simulator>
