<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>SphereSurface</Name>
	<Description>
		Model the surface of a sphere
		Data from Luigi Giaccari - http://www.advancedmcode.org/ 
	</Description>
	
	<InputParameters>
		<Parameter name="x" 	type="real"/>
		<Parameter name="y"	type="real"/>
		<Parameter name="z"	type="real"/>
	</InputParameters>
		
	<OutputParameters>
		<Parameter name="in" type="real" min="-1" max="1"/>
	</OutputParameters>
	
	<Options>
	  <Option key="model" value="SphereSurface.mat"/>
	</Options>
	
	<Implementation>
		<Executables>
			<!--Executable platform="matlab" batchMode="true" batchSize="100">inModel</Executable-->
			<Executable platform="matlab" batch="true" batchSize="100">inModel</Executable>
		</Executables>
		
		<DataFiles>
			<ScatteredDataFile id="default">SphereSurface.txt</ScatteredDataFile>
		</DataFiles>
		
	</Implementation>
</Simulator>
