<?xml version="1.0" encoding="ISO-8859-1" ?>
<Simulator>
	<Name>Welded Beam Design</Name>
	<Description>
		Welded beam design problem
		Hock and Schittkowski test set
		Solution according to
		G.V. Reklaitis, A. Ravindran, K.M. Ragsdell, Engineering Optimization Methods and Applications, Wiley, New York, 1983.
		h= 0.2444, l = 6.2187, t = 8.2915, b = 0.2444
		f*(x) = 2.38116
	</Description>
	
	<!-- The input parameters -->
	<InputParameters>
		<Parameter name="h" type="real" minimum="0.125" maximum="10"/>
		<Parameter name="l" type="real" minimum="0.1" maximum="10"/>
		<Parameter name="t" type="real" minimum="0.1" maximum="10"/>
		<Parameter name="b" type="real" minimum="0.1" maximum="10"/>

		<Constraint type="NonlinearConstraint" >
			<Option key="f" value="PrWc1" />
		</Constraint>
		<Constraint type="NonlinearConstraint" >
			<Option key="f" value="PrWc2" />
		</Constraint>
		<Constraint type="NonlinearConstraint" >
			<Option key="f" value="PrWc3" />
		</Constraint>
		<Constraint type="NonlinearConstraint" >
			<Option key="f" value="PrWc4" />
		</Constraint>
		<Constraint type="NonlinearConstraint" >
			<Option key="f" value="PrWc5" />
		</Constraint>
		<Constraint type="NonlinearConstraint" >
			<Option key="f" value="PrWc6" />
		</Constraint>

	</InputParameters>
	
	<!-- The output parameters -->
	<OutputParameters>
		<Parameter name="out" type="real"/>
	</OutputParameters>
	
	<!-- A simulator may have multiple implementations: as an executable, a 
	     java main class, a dataset, ...-->
	<Implementation>
		
		<Executables>
			<!-- An arch attribute may make sense here as well -->
			<Executable platform="matlab">PrWf</Executable>
		</Executables>
		
	</Implementation>
</Simulator>

