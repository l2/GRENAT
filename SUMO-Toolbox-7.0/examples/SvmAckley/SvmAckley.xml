<?xml version="1.0" encoding="ISO-8859-1" ?>
	<Simulator>
		<Name>SvmAckley</Name>
		<Description>
			Trains a RBF epsilon-SVM on the Ackley function for a given spread and regularization constant
			Returns the MSE on a testset.
		</Description>
		
		<!-- The input parameters -->
		<InputParameters>
			<Parameter name="spread" type="real"/>
			<Parameter name="C" type="real"/>
		</InputParameters>
			
		<!-- The output parameters -->
		<OutputParameters>
			<Parameter name="mse" type="real"/>
		</OutputParameters>
			
		<Implementation>
			
			<Executables>
				<Executable platform="matlab">SvmAckley</Executable>
			</Executables>
		
		</Implementation>
	</Simulator>
